FoamFile
{
	version     2.0;
	format      ascii;
	class       volScalarField;
	location    "0";
	object      k;
}

#include      "include/initialConditions";

dimensions    [0 2 -2 0 0 0 0];

internalField uniform $turbulentKE;

boundaryField
{
	#includeEtc	"caseDicts/setConstraintTypes"

	inlet
	{
		type	fixedValue;
		value	$internalField;
	}


	outlet
	{
		type	zeroGradient;
	}


	midPlane
	{
		type	symmetryPlane;
	}


	top
	{
		type	inletOutlet;
		inletValue	$internalField;
		value	$internalField;
	}


	hull
	{
		type	kLowReWallFunction;
		value	$internalField;
	}


	deck
	{
		type	kLowReWallFunction;
		value	$internalField;
	}

}
